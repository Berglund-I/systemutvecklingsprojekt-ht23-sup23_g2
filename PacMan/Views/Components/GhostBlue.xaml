<UserControl x:Class="PacMan.Views.Components.GhostBlue"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PacMan.Views.Components"
             xmlns:vm="clr-namespace:PacMan.ViewModels.Ghosts"
             d:DataContext="{d:DesignInstance vm:GhostViewModel}"
             x:Name="BlueGhost"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid Background="CornflowerBlue">
        <Ellipse HorizontalAlignment="Left" Height="10" Margin="10,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="10" Fill="Black"/>
        <Ellipse HorizontalAlignment="Left" Height="10" Margin="30,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="10" Fill="Black"/>
    </Grid>
</UserControl>

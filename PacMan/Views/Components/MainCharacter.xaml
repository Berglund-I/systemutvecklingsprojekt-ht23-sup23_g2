<UserControl x:Class="PacMan.Views.Components.MainCharacter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PacMan.Views.Components"
             xmlns:vm="clr-namespace:PacMan.ViewModels"
             mc:Ignorable="d" 
             x:Name="PlayableCharacter"
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
        <Ellipse Fill="Yellow" Height="30" Width="30"/>
        <Ellipse HorizontalAlignment="Left" Height="10" Width="10" Margin="30,10,0,0" Fill="Black" Stroke="Black" VerticalAlignment="Top" RenderTransformOrigin="0.066,0.612"/>
    </Grid>

</UserControl>

    </Resource>
    <Resource Include="Views\Images\7.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Views\Images\8.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>

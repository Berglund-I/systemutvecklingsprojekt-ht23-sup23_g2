  <ItemGroup>
    <Compile Remove="MainViewModels\**" />
    <Compile Remove="Views\Images2\**" />
    <EmbeddedResource Remove="MainViewModels\**" />
    <EmbeddedResource Remove="Views\Images2\**" />
    <None Remove="MainViewModels\**" />
    <None Remove="Views\Images2\**" />
    <Page Remove="MainViewModels\**" />
    <Page Remove="Views\Images2\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Resources\BackGroundMusic.wav" />
    <None Remove="Sounds\BackGroundMusic.wav" />
    <None Remove="Views\Images\0.png" />
    <None Remove="Views\Images\1.png" />
    <None Remove="Views\Images\2.png" />
    <None Remove="Views\Images\3.png" />
    <None Remove="Views\Images\4.png" />
    <None Remove="Views\Images\5.png" />
    <None Remove="Views\Images\6.png" />
    <None Remove="Views\Images\7.png" />
    <None Remove="Views\Images\8.png" />
